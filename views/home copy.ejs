 <% if(result ==0) { %> 
  There Are No Cars
<% } else { %>
  <% result.forEach(function(item) { %> 
      <div style="width:25%; display: inline-block;text-align: center; background-color: #555; min-height:200px; margin: 10px;border-radius: 8px;">
      <p style="color:antiquewhite;"> <%= item.id %> </p>
      <p style="color:antiquewhite;"><%= item.make %> </p>
      <p style="color:antiquewhite;"><%= item.model %> </p>
      <p style="color:antiquewhite;"><%= item.price %> </p>
      
      <p style="color:antiquewhite;text-decoration: none;"><a href="/car/<%= item.id %> " style="color:antiquewhite;text-decoration: none;">See This Car</a></p>
      <p style="color:antiquewhite;text-decoration: none;"><a href="/delete/<%= item.id %> " style="color:antiquewhite;text-decoration: none;">Delete This Car</a></p>
  </div>
      <% }); %>
 
<% } %> 


